page{
  height: 100%;
}
.search-box{
  position: relative;
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.search-box .search-input{
  background:#f1f1f1;
  font-size:32rpx;
  height:70rpx;
  line-height:70rpx;
  border-radius:40rpx;
  padding-left:70rpx;
}
.search-box .search-img{
  position:absolute;
  z-index:9;
  top:15rpx;
  width:45rpx;
  height:45rpx;
  left:20rpx;
}
.search-close{
  position:absolute;
  width:40rpx;
  height:40rpx;
  top:15rpx;
  right:20rpx;
  z-index: 99;
}
.clearbox{
  font-size: 26rpx;
  display: flex;
  justify-content: space-between;
  width: 90%;
  margin: auto;
  color: #5F5F5F;
}

.guanjianci{
  width: 90%;
  margin: 40rpx auto 0;
}
.guanjianci text{
  padding: 8rpx 16rpx;
  font-size: 22rpx;
  background: #f1f1f1;
  margin: 10rpx;
  border-radius: 6rpx;
  color: #5F5F5F;
  display: inline-block;
}

/* 下面是商品的样式 */
.product-box{
  display: flex;
  width: 100%;
  justify-content: space-between;
  align-items: center;
  border-bottom: 10rpx solid #f1f1f1;
}
.product-box .product-img{
  width: 200rpx;
  height: 200rpx;
  display: block;
  margin: auto;
}
.product-box .product-jsbox{
  padding: 44rpx 44rpx 44rpx 10rpx;
}
.product-box .product-jsbox .product-name{
  font-size: 30rpx;
  font-weight: 600;
}
.product-box .product-jsbox .product-beife{
  font-size: 24rpx;
  color: #E56059;
}
.product-box .product-jsbox .product-num{
  color: #9C9C9C;
  padding: 2rpx 5rpx;
  border: 1rpx solid #9C9C9C;
  border-radius: 20rpx;
  font-size: 20rpx;
}
.product-box .product-jsbox .price-box{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 30rpx;
}
.product-box .product-jsbox .price-box .t1{
  color: #E56059;
  font-size: 34rpx;
  font-weight: 600;
}
.product-box .product-jsbox .price-box .t2{
  color: #9C9C9C;
  font-size: 24rpx;
  padding-left: 10rpx;
  text-decoration: line-through;
}
.product-box .product-jsbox .price-box .index-shopping-car{
  width: 60rpx;
  height: 60rpx;
}
/* 上面是商品的样式 */

.nosearchnearbyCommunity{
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  width: 100%;
  height: 80%;
}
.nosearchnearbyCommunity image{
  width: 280rpx;
  height: 280rpx;
}
.nosearchnearbyCommunity view{
  padding: 10rpx 0;
  color: #ADADAD;
  font-size: 32rpx;
}